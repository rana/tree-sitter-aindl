          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "value"
        }
      ]
    },
    "property_key": {
      "type": "PATTERN",
      "value": "[A-Z][A-Z_0-9]*"
    },
    "value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "array"
        },
        {
          "type": "SYMBOL",
          "name": "block"
        }
      ]
    },
    "string": {
      "type": "PATTERN",
      "value": "\"[^\"]*\""
    },
    "number": {
      "type": "PATTERN",
      "value": "\\d+(\\.\\d+)?"
    },
    "array": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "value"
                },
                {
